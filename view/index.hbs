            dialog = document.querySelector('dialog'),
            backDialog = document.getElementById('back'),
            getError = document.getElementById('getError'),
            formIn = document.querySelector('#formIn'),
            formUp = document.querySelector('#formUp'),
            inpArquivo = document.querySelector('#arquivo'),
            formUpload = document.querySelector('#form-up');

        /*
        formSearch.addEventListener('submit', (ev) => {
