        */

        searchIcon.addEventListener('click', function () {
            search.className = 'show-search';
            bloco.className = 'none';
        });


        document.querySelectorAll('.bt-sign-up').forEach(bt => bt.addEventListener('click', function () {
            bloco.className += ' bloco-log';
            blocoI.className += ' none';
            blocoLoginIn.className = 'bloco-login none';
            blocoLoginUp.className = 'bloco-login';
        }));

        document.querySelectorAll('.bt-sign-in').forEach(bt => bt.addEventListener('click', function () {
            bloco.className += ' bloco-log';
            blocoI.className += ' none';
            blocoLoginUp.className = 'bloco-login-up none';
            blocoLoginIn.className = 'bloco-login';
        }));

        document.querySelector('#Sign-out').addEventListener('click', function () {
            localStorage.removeItem('admin');
            localStorage.removeItem('username');
            document.location.reload(true);
        });


        function btSubmitSignIn() {
            data = { email: formIn.elements['emailIn'].value, password: formIn.elements['passwordIn'].value };
            if (formIn.elements['emailIn'].value.length > 2 && formIn.elements['passwordIn'].value.length > 2) {
                server('POST', './login', data);
            } else if (formIn.elements['emailIn'].value.length < 3 && formIn.elements['passwordIn'].value.length < 3) {
                backDialog.className = 'backDialog';
                getError.innerHTML = 'The email and password must contain a minimum of 3 characters';
                dialogShow();
            } else if (formIn.elements['emailIn'].value.length < 3) {
                backDialog.className = 'backDialog';
                getError.innerHTML = 'The email must contain a minimum of 3 characters';
                dialogShow();
            } else {
                backDialog.className = 'backDialog';
                getError.innerHTML = 'The password must contain a minimum of 3 characters';
                dialogShow();
            }

        };

        function btSubmitSignUp() {
            data = { name: formUp.elements['name'].value, username: formUp.elements['username'].value, email: formUp.elements['email'].value, password: formUp.elements['password'].value, admin: formUp.elements['admin'].checked };
            if (formUp.elements['name'].value.length > 2 && formUp.elements['username'].value.length > 2 && formUp.elements['email'].value.length > 2 && formUp.elements['password'].value.length > 2) {
                server('POST', './new_user', data);
            } else if (formUp.elements['email'].value.length < 3 && formUp.elements['password'].value.length < 3) {
                backDialog.className = 'backDialog';
                getError.innerHTML = 'The email and password must contain a minimum of 3 characters';
                dialogShow();
            } else if (formUp.elements['email'].value.length < 3) {
                backDialog.className = 'backDialog';
                getError.innerHTML = 'The email must contain a minimum of 3 characters';
                dialogShow();
            } else {
                backDialog.className = 'backDialog';
                getError.innerHTML = 'The password must contain a minimum of 3 characters';
                dialogShow();
            }

        };

        function dialogShow() {
            dialog.setAttribute('style', 'display: flex;');
        }

        function btFormUp() {
            if (formUpload.elements['arquivo'].value != '') {
                if (formUpload.elements['arquivo'].value.toLowerCase().includes('mp4')) {
                    formUpload.setAttribute('action', './upload-video');
                    formUpload.submit();
                }
                else {
                    formUpload.setAttribute('action', './upload-image');
                    formUpload.submit();
                }
            }
        }

        function btCloseDialog(data) {
            dialog.removeAttribute('style');
            backDialog.removeAttribute('class');
            document.location.reload(true);
        }

        a2();

        function a2() {
            if (localStorage.admin == undefined) {
                document.querySelector('.admin-true').className = 'none';
            }
            else if (localStorage.admin == 'true') {
                search.className = 'show-search';
                bloco.className = 'none';
                bloco.className = 'none';
                blocoI.className = 'bloco-i';
                blocoI.className = 'none';
                blocoLoginIn.className = 'none';
                blocoLoginUp.className = 'none';
                btUpload.innerHTML = localStorage.username;
                btSign.style.display = 'none';
                avatarShow.removeAttribute('style');
                /// avatar.setAttribute('src', JSON.parse(localStorage.getItem('user')).avatar);
                searchIcon.removeAttribute('style');
            }
            else {
                search.className = 'show-search';
                bloco.className = 'none';
                bloco.className = 'none';
                blocoI.className = 'bloco-i';
                blocoI.className = 'none';
                blocoLoginIn.className = 'none';
                blocoLoginUp.className = 'none';
                btUpload.innerHTML = localStorage.username;
                btSign.style.display = 'none';
                avatarShow.removeAttribute('style');
                //avatar.setAttribute('src', JSON.parse(localStorage.getItem('user')).avatar);
                searchIcon.removeAttribute('style');
                document.querySelector('.admin-true').className = '.admin-true';
                document.querySelector('.upload').className = 'none';
            }
        };


        function server(metodo, url, data) {
            request = new XMLHttpRequest();
            request.open(metodo, url, true);

            request.responseType = 'json';
            request.setRequestHeader('Content-Type', 'application/json');

            request.onload = function () {
                if (request.status >= 200) {
                    backDialog.className = 'backDialog';
                    getError.innerHTML = request.response.error;
                    dialogShow();
                    if (request.status != 201) {
                        localStorage.setItem('admin', request.response.admin);
                        localStorage.setItem('username', request.response.username);
                    }

                }
            };
            request.send(JSON.stringify(data));
        };

    </script>
